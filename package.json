  "private": true,
  "scripts": {
    "dev": "npm run dev:core & npm run dev:proxy",
    "dev:core": "next dev -p 3000",
    "dev:proxy": "local-ssl-proxy --key localhost-key.pem --cert localhost.pem --source 3001 --target 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --check \"src/**/*.{ts,tsx}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "storybook": "storybook dev -p 6006 --https --ssl-cert ./localhost.pem --ssl-key ./localhost-key.pem ",
    "build-storybook": "storybook build"
  },
